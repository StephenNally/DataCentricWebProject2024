<h1>Lecturers</h1>
<a href="/">Home</a>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Department</th>
    <th>Actions</th>
  </tr>
  <% lecturers.forEach(lecturer => { %>
    <tr>
      <td><%= lecturer._id %></td>
      <td><%= lecturer.name %></td>
      <td><%= lecturer.did %></td>
      <td><a href="/lecturers/delete/<%= lecturer._id %>">Delete</a></td>
    </tr>
  <% }); %>
</table>
